#include <iostream>
#include <queue>
using namespace std;

class StackTwoQueues {
    queue<int> q1, q2;
public:
    void push(int x) {
        // push into q2
        q2.push(x);

        // move all from q1 to q2
        while (!q1.empty()) {
            q2.push(q1.front());
            q1.pop();
        }

        // swap q1 and q2
        swap(q1, q2);
    }

    void pop() {
        if (q1.empty()) {
            cout << "Stack Underflow!\n";
            return;
        }
        cout << q1.front() << " popped.\n";
        q1.pop();
    }

    int top() {
        if (q1.empty()) {
            cout << "Stack is empty.\n";
            return -1;
        }
        return q1.front();
    }

    bool isEmpty() {
        return q1.empty();
    }
};

int main() {
    StackTwoQueues st;
    st.push(10);
    st.push(20);
    st.push(30);
    cout << "Top = " << st.top() << endl;
    st.pop();
    cout << "Top = " << st.top() << endl;
    return 0;
}
