#include <iostream>
#include <queue>
#include <vector>
using namespace std;

int main() {
    string s;
    cout << "Enter string (continuous stream, e.g. aabc): ";
    cin >> s;

    vector<int> freq(256, 0);
    queue<char> q;

    cout << "First non-repeating after each character:\n";
    for (int i = 0; i < (int)s.length(); i++) {
        char ch = s[i];
        freq[ch]++;
        q.push(ch);

        // remove all repeating chars from front
        while (!q.empty() && freq[q.front()] > 1) {
            q.pop();
        }

        if (q.empty())
            cout << "-1 ";
        else
            cout << q.front() << " ";
    }

    cout << endl;
    return 0;
}
