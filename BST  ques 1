#include <iostream>
using namespace std;

struct Node {
    int data;
    Node *left, *right;
    Node(int x) : data(x), left(NULL), right(NULL) {}
};

void preorder(Node* root) {          // Root, Left, Right
    if (!root) return;
    cout << root->data << " ";
    preorder(root->left);
    preorder(root->right);
}

