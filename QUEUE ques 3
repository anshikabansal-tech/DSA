#include <iostream>
#include <queue>
using namespace std;

int main() {
    queue<int> q;
    int n;

    cout << "Enter even number of elements in queue: ";
    cin >> n;

    if (n % 2 != 0) {
        cout << "Size must be even.\n";
        return 0;
    }

    cout << "Enter " << n << " elements:\n";
    for (int i = 0; i < n; i++) {
        int x;
        cin >> x;
        q.push(x);
    }

    int half = n / 2;
    queue<int> firstHalf;

    // move first half
    for (int i = 0; i < half; i++) {
        firstHalf.push(q.front());
        q.pop();
    }

    // interleave
    while (!firstHalf.empty()) {
        q.push(firstHalf.front());
        firstHalf.pop();

        q.push(q.front());
        q.pop();
    }

    cout << "Interleaved queue: ";
    while (!q.empty()) {
        cout << q.front() << " ";
        q.pop();
    }
    cout << endl;

    return 0;
}
